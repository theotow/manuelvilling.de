{
    "name": "frontend-next",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "eslint ./",
        "test": "jest",
        "lint-fix": "yarn lint -- --fix",
        "deploy": "../../node_modules/.bin/vercel -A ./frontend-next.json -b TELEGRAM_CHAT_ID=@telegram_chat_id -b TELEGRAM_TOKEN=@telegram_token",
        "deploy-prod": "../../node_modules/.bin/vercel -A ./frontend-next.json --prod -b TELEGRAM_CHAT_ID=@telegram_chat_id -b TELEGRAM_TOKEN=@telegram_token",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook"
    },
    "jest": {
        "testURL": "http://localhost/",
        "moduleNameMapper": {
            "\\.(svg|png|woff)$": "<rootDir>/tests/__mocks__/empty-module.js"
        },
        "setupFiles": [
            "<rootDir>/jest.setup.js"
        ],
        "transform": {
            "^.+\\.js?$": "babel-jest"
        },
        "testPathIgnorePatterns": [
            "/.next/",
            "/node_modules/"
        ],
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ],
        "coveragePathIgnorePatterns": [
            "/node_modules/",
            "/.next/",
            "/coverage/"
        ]
    },
    "dependencies": {
        "@apollo/client": "^3.1.5",
        "@apollo/react-components": "^4.0.0",
        "@apollo/react-ssr": "^4.0.0",
        "apollo-boost": "^0.4.7",
        "apollo-cache": "^1.3.2",
        "apollo-client": "^2.6.4",
        "apollo-link": "^1.2.12",
        "apollo-server-micro": "2.17.0",
        "apollo-utilities": "^1.3.2",
        "graphql": "^15.3.0",
        "graphql-tag": "^2.10.1",
        "micro-cors": "^0.1.1",
        "mobile-detect": "^1.4.4",
        "next": "latest",
        "node-fetch": "^2.6.7",
        "react": "^16.6.3",
        "react-animations": "^1.0.0",
        "react-dom": "^16.6.3",
        "react-router": "^5.1.2",
        "react-router-config": "^5.1.1",
        "react-router-dom": "^5.1.2",
        "request": "^2.87.0",
        "request-promise": "^4.2.2",
        "rxjs": "^6.5.3",
        "rxjs-compat": "^6.5.3",
        "storybook": "^6.0.21",
        "styled-components": "4.4.1",
        "telegraf": "^3.34.1"
    },
    "devDependencies": {
        "@babel/core": "^7.7.7",
        "@storybook/addon-actions": "^6.0.21",
        "@storybook/addon-console": "^1.2.1",
        "@storybook/addon-links": "^6.0.21",
        "@storybook/addon-storysource": "^6.0.21",
        "@storybook/addons": "^6.0.21",
        "@storybook/react": "^6.0.21",
        "@testing-library/react": "^9.4.0",
        "babel-eslint": "^10.0.3",
        "babel-loader": "^8.0.6",
        "dotenv": "^8.2.0",
        "eslint": "^7.9.0",
        "eslint-plugin-react": "^7.17.0",
        "jest": "^26.4.2",
        "next-transpile-modules": "^2.3.1",
        "url-loader": "^1.1.2"
    }
}
